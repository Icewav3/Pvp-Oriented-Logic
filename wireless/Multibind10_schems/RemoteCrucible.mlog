#pyra
sensor m1_sa mixer1 @sand
sensor m1_co mixer1 @coal
sensor m1_le mixer1 @lead
sensor m2_sa mixer2 @sand
sensor m2_co mixer2 @coal
sensor m2_le mixer2 @lead
#cruc
sensor c1_sa crucible1 @sand
sensor c1_co crucible1 @coal
sensor c2_sa crucible2 @sand
sensor c2_co crucible2 @coal
#min calc cruc (top) unloader 3-4
op min c_sa c1_sa c2_sa
op min c_co c1_co c2_co
#min calc mix (bottom) unloader 1-2
op min m_sa m1_sa m2_sa
op min m_co m1_co m2_co
op min m_le m1_le m2_le

jump cruc_co lessThan c_co 22

cruc_sa:
    control configure unloader3 @sand
    control configure unloader4 @sand
    jump mixer_start always
cruc_co:
    control configure unloader3 @coal
    control configure unloader4 @coal
mixer_start:
    jump bottom_co lessThan m_co 8
    jump bottom_le lessThan m_le 6
bottom_sa:
    control configure unloader1 @sand
    control configure unloader2 @sand
    jump on always
bottom_co:
    control configure unloader1 @coal
    control configure unloader2 @coal
    jump on always
bottom_le:
    control configure unloader1 @lead
    control configure unloader2 @lead

    on:
    jump boot2 notEqual init null
    on2:
    printflush message1
    set init 1
    set target container1
    set u1item @lead
    set u2item @sand
    set u3item @silicon
    set u4item @coal
    set u5item @sand
    set u6item @silicon
    set u7item @coal
    set u8item @silicon
    set u9item @sand
    set u10item @silicon
    set u1mode target
    set u2mode target
    set u3mode core
    set u4mode target
    set u5mode target
    set u6mode core
    set u7mode target
    set u8mode core
    set u9mode target
    set u10mode core
    boot2:
    jump on2 strictEqual target null
    set prevscale scale
    set prevconfig config
    sensor config sorter1 @config
    sensor scale @this @timescale
    jump unbind notEqual config prevconfig
    jump u_type notEqual scale prevscale
    sensor shit core @dead
    jump boot equal shit true
    jump start notEqual config null
    unbind:
    set u1 null
    set u2 null
    set u3 null
    set u4 null
    set u5 null
    set u6 null
    set u7 null
    set u8 null
    set u9 null 
    set u10 null
    u_type:
    jump boot2 strictEqual config null
    jump flare equal config @copper
    jump mono equal config @lead
    jump atrax equal config @metaglass
    jump poly equal config @graphite
    jump mega equal config @thorium
    jump zenith equal config @titanium
    jump unbind equal config @null
    end

    flare:
    set unit_type @flare
    set unit_icon ""
    set u_cap 10
    set units 9
    op mul units units scale
    jump start always x false
    mono:
    set unit_type @mono
    set unit_icon ""
    set u_cap 20
    set units 9
    op mul units units scale
    jump start always x false
    atrax:
    set unit_type @atrax
    set unit_icon ""
    set u_cap 50
    set units 4
    op mul units units scale
    jump start always x false
    poly:
    set unit_type @poly
    set unit_icon ""
    set u_cap 30
    set units 6
    op mul units units scale
    jump start always x false
    mega:
    set unit_type @mega
    set unit_icon ""
    set u_cap 60

        set key8 key
        end
        u9set:
        jump done lessThan units 9
        jump u10set notEqual u9 null
        set u9 @unit
        set key9 key
        end
        u10set:
        jump done lessThan units 10
        jump ubind_end notEqual u10 null
        set u10 @unit
        set key10 key
        end
        ubind_end:
    done:
